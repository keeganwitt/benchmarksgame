            <span class="hl kwa">while</span> <span class="hl opt">((</span>rNew <span class="hl opt">!=</span> toFill<span class="hl opt">[</span><span class="hl num">0</span><span class="hl opt">]) &amp;&amp; (</span>rNew <span class="hl opt">!=</span> region<span class="hl opt">));</span>

            <span class="hl slc">// subtract empty region from board</span>
