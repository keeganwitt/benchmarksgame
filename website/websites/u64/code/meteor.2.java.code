            <span class="hl kwb">int</span> mask <span class="hl opt">=</span> <span class="hl kwd">getMask</span><span class="hl opt">(</span>lastRow<span class="hl opt">);</span>
            <span class="hl kwb">int</span> isOdd <span class="hl opt">=</span> row <span class="hl opt">&amp;</span> <span class="hl num">1</span><span class="hl opt">;</span>

