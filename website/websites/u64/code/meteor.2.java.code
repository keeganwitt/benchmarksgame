                m_pieces<span class="hl opt">[</span>i<span class="hl opt">] =</span> <span class="hl kwa">new</span> <span class="hl kwd">SPiece</span><span class="hl opt">();</span>
        <span class="hl opt">}</span>
        <span class="hl kwd">Soln</span> <span class="hl opt">(</span><span class="hl kwb">int</span> fillVal<span class="hl opt">) {</span>
            <span class="hl kwd">init</span><span class="hl opt">();</span>
            m_nPiece <span class="hl opt">=</span> <span class="hl num">0</span><span class="hl opt">;</span>
