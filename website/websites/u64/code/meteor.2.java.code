            <span class="hl kwb">int</span> iInfo <span class="hl opt">=</span> boardVec <span class="hl opt">&amp; ((</span><span class="hl num">1</span> <span class="hl opt">&lt;&lt;</span> <span class="hl num">2</span> <span class="hl opt">*</span> N_COL<span class="hl opt">) -</span> <span class="hl num">1</span><span class="hl opt">);</span>
