        <span class="hl opt">}</span>
        <span class="hl kwa">static</span> <span class="hl kwb">int</span> <span class="hl kwd">calcBadIslands</span><span class="hl opt">(</span><span class="hl kwb">int</span> boardVec<span class="hl opt">,</span> <span class="hl kwb">int</span> row<span class="hl opt">)</span>
        <span class="hl opt">{</span>
            <span class="hl kwb">int</span><span class="hl opt">[]</span> toFill <span class="hl opt">= {</span>~boardVec<span class="hl opt">};</span>
            <span class="hl kwa">if</span> <span class="hl opt">((</span>row <span class="hl opt">&amp;</span> <span class="hl num">1</span><span class="hl opt">) !=</span> <span class="hl num">0</span><span class="hl opt">) {</span>
                row<span class="hl opt">--;</span>
