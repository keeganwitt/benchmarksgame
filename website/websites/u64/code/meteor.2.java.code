        <span class="hl kwa">static</span> <span class="hl kwb">void</span> <span class="hl kwd">genOrientation</span><span class="hl opt">(</span><span class="hl kwb">int</span> vec<span class="hl opt">,</span> <span class="hl kwb">int</span> iOrient<span class="hl opt">,</span> Piece target<span class="hl opt">)</span>
