<span class="hl kwa">extern fun</span> free_atm <span class="hl opt">{</span>n<span class="hl opt">:</span>nat<span class="hl opt">} {</span>l<span class="hl opt">:</span>addr<span class="hl opt">}</span>
  <span class="hl opt">(</span>pf<span class="hl opt">:</span> bytes n &#64; l | p<span class="hl opt">:</span> ptr l<span class="hl opt">):</span> void <span class="hl opt">=</span> <span class="hl str">&quot;free_atm&quot;</span>

