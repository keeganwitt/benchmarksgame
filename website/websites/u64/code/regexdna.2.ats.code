<span class="hl opt">}</span>

<span class="hl kwa">static</span> the_nticket <span class="hl opt">;</span> <span class="hl kwa">static</span> the_nthread <span class="hl opt">;</span>
<span class="hl kwa">static</span> pthread_mutex_t mutex_nticket <span class="hl opt">=</span> <span class="hl kwd">PTHREAD_MUTEX_INITIALIZER</span><span class="hl opt">;</span>
<span class="hl kwa">static</span> pthread_mutex_t mutex_nthread <span class="hl opt">=</span> <span class="hl kwd">PTHREAD_MUTEX_INITIALIZER</span><span class="hl opt">;</span>

<span class="hl kwd">ATSinline</span><span class="hl opt">()</span>
