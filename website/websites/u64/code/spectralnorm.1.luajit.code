<span class="hl kwa">end</span>

<span class="hl kwa">local function</span> <span class="hl kwd">AtAv</span><span class="hl sym">(</span>x<span class="hl sym">,</span> y<span class="hl sym">,</span> t<span class="hl sym">,</span> N<span class="hl sym">)</span>
  <span class="hl kwd">Av</span><span class="hl sym">(</span>x<span class="hl sym">,</span> t<span class="hl sym">,</span> N<span class="hl sym">)</span>
