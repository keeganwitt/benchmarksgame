<span class="hl kwa">local function</span> <span class="hl kwd">A</span><span class="hl sym">(</span>i<span class="hl sym">,</span> j<span class="hl sym">)</span>
  <span class="hl kwa">local</span> ij <span class="hl sym">=</span> i<span class="hl sym">+</span>j<span class="hl sym">-</span><span class="hl num">1</span>
  <span class="hl kwa">return</span> <span class="hl num">1.0</span> <span class="hl sym">/ (</span>ij <span class="hl sym">* (</span>ij<span class="hl sym">-</span><span class="hl num">1</span><span class="hl sym">) *</span> <span class="hl num">0.5</span> <span class="hl sym">+</span> i<span class="hl sym">)</span>
<span class="hl kwa">end</span>

<span class="hl kwa">local function</span> <span class="hl kwd">Av</span><span class="hl sym">(</span>x<span class="hl sym">,</span> y<span class="hl sym">,</span> N<span class="hl sym">)</span>
