    | <span class="hl kwa">otherwise</span> <span class="hl opt">= (</span>makeInfo <span class="hl opt">(</span>mask piece x1 y1 <span class="hl num">0</span><span class="hl opt">)</span> w1 h1<span class="hl opt">,</span> makeInfo <span class="hl opt">(</span>mask piece x2 <span class="hl opt">(</span>y2<span class="hl opt">+</span><span class="hl num">1</span><span class="hl opt">)</span> <span class="hl num">0</span> <span class="hl opt">&gt;&gt;</span> N_COL<span class="hl opt">)</span> w2 h2<span class="hl opt">)</span>
  <span class="hl kwa">where</span>
    mask <span class="hl opt">[]</span> x y m <span class="hl opt">=</span> m bitor <span class="hl opt">(</span><span class="hl num">1</span> <span class="hl opt">&lt;&lt;</span> cellAt x y<span class="hl opt">)</span>
