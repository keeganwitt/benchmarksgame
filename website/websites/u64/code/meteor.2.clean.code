        top_mask <span class="hl opt">=</span> mask <span class="hl opt">&lt;&lt; (</span>N_COL<span class="hl opt">*(</span><span class="hl num">5</span> <span class="hl opt">-</span> dy<span class="hl opt">))</span>
        ok <span class="hl opt">=</span>     sum <span class="hl opt">[</span><span class="hl num">1</span><span class="hl opt">&lt;&lt;</span>x <span class="hl esc">\\</span> x <span class="hl opt">&lt;- [</span>x0<span class="hl opt">+</span><span class="hl num">1</span><span class="hl opt">..</span>xmax<span class="hl opt">-</span><span class="hl num">1</span><span class="hl opt">]]</span> <span class="hl slc">// all but edges  </span>
