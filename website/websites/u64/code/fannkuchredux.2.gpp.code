    <span class="hl kwb">void</span> <span class="hl kwd">count_flips</span><span class="hl opt">(</span>R<span class="hl opt">&amp;</span>r<span class="hl opt">,</span> <span class="hl kwb">int</span> i<span class="hl opt">)</span>
    <span class="hl opt">{</span><span class="hl slc">//performance magic happen here. </span>
