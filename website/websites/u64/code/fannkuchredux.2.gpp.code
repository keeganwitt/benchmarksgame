            <span class="hl opt">}</span><span class="hl kwa">while</span><span class="hl opt">(</span> f <span class="hl opt">=</span> <span class="hl kwd">p0</span><span class="hl opt">(</span>p<span class="hl opt">) );</span>
        <span class="hl opt">}</span>
        <span class="hl kwb">int</span> total_flips <span class="hl opt">=</span> flips<span class="hl opt">;</span>
