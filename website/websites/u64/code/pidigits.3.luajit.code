<span class="hl kwa">local</span> g <span class="hl sym">= {};</span> require<span class="hl str">&quot;c-gmp&quot;</span><span class="hl sym">(</span>g<span class="hl sym">, {})</span>
<span class="hl kwa">local</span> add<span class="hl sym">,</span> mul<span class="hl sym">,</span> submul <span class="hl sym">=</span> g<span class="hl sym">.</span>mpz_add<span class="hl sym">,</span> g<span class="hl sym">.</span>mpz_mul_si<span class="hl sym">,</span> g<span class="hl sym">.</span>mpz_submul_ui
<span class="hl kwa">local</span> mul2x<span class="hl sym">,</span> div<span class="hl sym">,</span> cmp <span class="hl sym">=</span> g<span class="hl sym">.</span>mpz_mul_2exp<span class="hl sym">,</span> g<span class="hl sym">.</span>mpz_fdiv_qr<span class="hl sym">,</span> g<span class="hl sym">.</span>mpz_cmp
<span class="hl kwa">local</span> init<span class="hl sym">,</span> get<span class="hl sym">,</span> <span class="hl kwb">write</span> <span class="hl sym">=</span> g<span class="hl sym">.</span>mpz_init_set_d<span class="hl sym">,</span> g<span class="hl sym">.</span>mpz_get_d<span class="hl sym">,</span> io<span class="hl sym">.</span><span class="hl kwb">write</span>

<span class="hl kwa">local</span> N <span class="hl sym">=</span> <span class="hl kwb">tonumber</span><span class="hl sym">(</span>arg <span class="hl kwa">and</span> arg<span class="hl sym">[</span><span class="hl num">1</span><span class="hl sym">])</span> <span class="hl kwa">or</span> <span class="hl num">100</span>
<span class="hl kwa">local</span> i<span class="hl sym">,</span> n<span class="hl sym">,</span> a<span class="hl sym">,</span> d<span class="hl sym">,</span> t<span class="hl sym">,</span> u <span class="hl sym">=</span> <span class="hl num">0</span><span class="hl sym">,</span> <span class="hl kwd">init</span><span class="hl sym">(</span><span class="hl num">1</span><span class="hl sym">),</span> <span class="hl kwd">init</span><span class="hl sym">(</span><span class="hl num">0</span><span class="hl sym">),</span> <span class="hl kwd">init</span><span class="hl sym">(</span><span class="hl num">1</span><span class="hl sym">),</span> <span class="hl kwd">init</span><span class="hl sym">(</span><span class="hl num">0</span><span class="hl sym">),</span> <span class="hl kwd">init</span><span class="hl sym">(</span><span class="hl num">0</span><span class="hl sym">)</span>
<span class="hl kwa">for</span> k<span class="hl sym">=</span><span class="hl num">1</span><span class="hl sym">,</span><span class="hl num">1000000</span> <span class="hl kwa">do</span>
  <span class="hl kwd">mul2x</span><span class="hl sym">(</span>n<span class="hl sym">,</span> <span class="hl num">1</span><span class="hl sym">,</span> t<span class="hl sym">)</span> <span class="hl kwd">mul</span><span class="hl sym">(</span>n<span class="hl sym">,</span> k<span class="hl sym">,</span> n<span class="hl sym">)</span> <span class="hl kwd">add</span><span class="hl sym">(</span>a<span class="hl sym">,</span> t<span class="hl sym">,</span> a<span class="hl sym">)</span> <span class="hl kwd">mul</span><span class="hl sym">(</span>a<span class="hl sym">,</span> k<span class="hl sym">+</span>k<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">,</span> a<span class="hl sym">)</span> <span class="hl kwd">mul</span><span class="hl sym">(</span>d<span class="hl sym">,</span> k<span class="hl sym">+</span>k<span class="hl sym">+</span><span class="hl num">1</span><span class="hl sym">,</span> d<span class="hl sym">)</span>
  <span class="hl kwa">if</span> <span class="hl kwd">cmp</span><span class="hl sym">(</span>a<span class="hl sym">,</span> n<span class="hl sym">) &gt;=</span> <span class="hl num">0</span> <span class="hl kwa">then</span>
    <span class="hl kwd">mul2x</span><span class="hl sym">(</span>n<span class="hl sym">,</span> <span class="hl num">1</span><span class="hl sym">,</span> t<span class="hl sym">)</span> <span class="hl kwd">add</span><span class="hl sym">(</span>t<span class="hl sym">,</span> n<span class="hl sym">,</span> t<span class="hl sym">)</span> <span class="hl kwd">add</span><span class="hl sym">(</span>t<span class="hl sym">,</span> a<span class="hl sym">,</span> t<span class="hl sym">)</span> <span class="hl kwd">div</span><span class="hl sym">(</span>t<span class="hl sym">,</span> d<span class="hl sym">,</span> t<span class="hl sym">,</span> u<span class="hl sym">)</span> <span class="hl kwd">add</span><span class="hl sym">(</span>u<span class="hl sym">,</span> n<span class="hl sym">,</span> u<span class="hl sym">)</span>
    <span class="hl kwa">if</span> <span class="hl kwd">cmp</span><span class="hl sym">(</span>d<span class="hl sym">,</span> u<span class="hl sym">) &gt;</span> <span class="hl num">0</span> <span class="hl kwa">then</span>
      <span class="hl kwa">local</span> y <span class="hl sym">=</span> <span class="hl kwd">get</span><span class="hl sym">(</span>t<span class="hl sym">)</span>
      <span class="hl kwb">write</span><span class="hl sym">(</span>y<span class="hl sym">);</span> i <span class="hl sym">=</span> i <span class="hl sym">+</span> <span class="hl num">1</span><span class="hl sym">;</span> <span class="hl kwa">if</span> i <span class="hl sym">%</span> <span class="hl num">10</span> <span class="hl sym">==</span> <span class="hl num">0</span> <span class="hl kwa">then</span> <span class="hl kwb">write</span><span class="hl sym">(</span><span class="hl str">&quot;</span><span class="hl esc">\t</span><span class="hl str">:&quot;</span><span class="hl sym">,</span> i<span class="hl sym">,</span> <span class="hl str">&quot;</span><span class="hl esc">\n</span><span class="hl str">&quot;</span><span class="hl sym">)</span> <span class="hl kwa">end</span>
      <span class="hl kwa">if</span> i <span class="hl sym">&gt;=</span> N <span class="hl kwa">then break end</span>
      <span class="hl kwd">submul</span><span class="hl sym">(</span>a<span class="hl sym">,</span> d<span class="hl sym">,</span> y<span class="hl sym">)</span> <span class="hl kwd">mul</span><span class="hl sym">(</span>a<span class="hl sym">,</span> <span class="hl num">10</span><span class="hl sym">,</span> a<span class="hl sym">)</span> <span class="hl kwd">mul</span><span class="hl sym">(</span>n<span class="hl sym">,</span> <span class="hl num">10</span><span class="hl sym">,</span> n<span class="hl sym">)</span>
    <span class="hl kwa">end</span>
  <span class="hl kwa">end</span>
<span class="hl kwa">end</span>
<span class="hl kwa">if</span> i <span class="hl sym">%</span> <span class="hl num">10</span> <span class="hl sym">~=</span> <span class="hl num">0</span> <span class="hl kwa">then</span> <span class="hl kwb">write</span><span class="hl sym">(</span><span class="hl kwb">string</span><span class="hl sym">.</span><span class="hl kwd">rep</span><span class="hl sym">(</span><span class="hl str">&quot; &quot;</span><span class="hl sym">,</span> <span class="hl num">10</span> <span class="hl sym">-</span> N <span class="hl sym">%</span> <span class="hl num">10</span><span class="hl sym">),</span> <span class="hl str">&quot;</span><span class="hl esc">\t</span><span class="hl str">:&quot;</span><span class="hl sym">,</span> N<span class="hl sym">,</span> <span class="hl str">&quot;</span><span class="hl esc">\n</span><span class="hl str">&quot;</span><span class="hl sym">)</span> <span class="hl kwa">end</span>
