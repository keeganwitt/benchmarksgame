  <span class="hl kwa">for</span> <span class="hl opt">(</span>i <span class="hl opt">=</span> <span class="hl num">0</span><span class="hl opt">;</span> i <span class="hl opt">&lt;</span> NBODIES<span class="hl opt">;</span> i<span class="hl opt">++) {</span>
    b <span class="hl opt">= &amp;(</span>bodies<span class="hl opt">[</span>i<span class="hl opt">]);</span>
    <span class="hl com">/* b-&gt;mass*(vx*vx + vy*vy) | b-&gt;mass*(vz*vz + 0*0) */</span>
    sq <span class="hl opt">=</span> b<span class="hl opt">-&gt;</span>massmass <span class="hl opt">*</span> <span class="hl kwd">__builtin_ia32_haddpd</span><span class="hl opt">(</span>b<span class="hl opt">-&gt;</span>vxvy<span class="hl opt">*</span>b<span class="hl opt">-&gt;</span>vxvy<span class="hl opt">,</span> b<span class="hl opt">-&gt;</span>vz00<span class="hl opt">*</span>b<span class="hl opt">-&gt;</span>vz00<span class="hl opt">);</span>
    sq <span class="hl opt">=</span> <span class="hl kwd">__builtin_ia32_haddpd</span><span class="hl opt">(</span>sq<span class="hl opt">,</span> sq<span class="hl opt">);</span>
    e <span class="hl opt">+=</span> half <span class="hl opt">*</span> sq<span class="hl opt">;</span>
    <span class="hl kwa">for</span> <span class="hl opt">(</span>j <span class="hl opt">=</span> i <span class="hl opt">+</span> <span class="hl num">1</span><span class="hl opt">;</span> j <span class="hl opt">&lt;</span> NBODIES<span class="hl opt">;</span> j<span class="hl opt">++) {</span>
      b2 <span class="hl opt">= &amp;(</span>bodies<span class="hl opt">[</span>j<span class="hl opt">]);</span>
      dxdy <span class="hl opt">=</span> b<span class="hl opt">-&gt;</span>xy <span class="hl opt">-</span> b2<span class="hl opt">-&gt;</span>xy<span class="hl opt">;</span>
      dz00 <span class="hl opt">=</span> b<span class="hl opt">-&gt;</span>z0 <span class="hl opt">-</span> b2<span class="hl opt">-&gt;</span>z0<span class="hl opt">;</span>
      <span class="hl com">/* b-&gt;mass*(vx*vx + vy*vy) | b-&gt;mass*(vz*vz + 0*0) */</span>
      distance <span class="hl opt">=</span> <span class="hl kwd">__builtin_ia32_haddpd</span><span class="hl opt">(</span>dxdy<span class="hl opt">*</span>dxdy<span class="hl opt">,</span> dz00<span class="hl opt">*</span>dz00<span class="hl opt">);</span>
      distance <span class="hl opt">=</span> <span class="hl kwd">__builtin_ia32_sqrtpd</span><span class="hl opt">(</span><span class="hl kwd">__builtin_ia32_haddpd</span><span class="hl opt">(</span>distance<span class="hl opt">,</span>distance<span class="hl opt">));</span>
      e <span class="hl opt">-= (</span>b<span class="hl opt">-&gt;</span>massmass <span class="hl opt">*</span> b2<span class="hl opt">-&gt;</span>massmass<span class="hl opt">) /</span> distance<span class="hl opt">;</span>
    <span class="hl opt">}</span>
  <span class="hl opt">}</span>
