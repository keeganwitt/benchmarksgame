<span class="hl kwb">static double</span> <span class="hl kwd">energy</span><span class="hl opt">()</span>
<span class="hl opt">{</span>
  v2df e <span class="hl opt">= {</span><span class="hl num">0.0</span><span class="hl opt">,</span> <span class="hl num">0.0</span><span class="hl opt">};</span>
  v2df half <span class="hl opt">= {</span><span class="hl num">0.5</span><span class="hl opt">,</span> <span class="hl num">0.5</span><span class="hl opt">};</span>
  v2df sq<span class="hl opt">,</span> dxdy<span class="hl opt">,</span> dz00<span class="hl opt">,</span> distance<span class="hl opt">;</span>
