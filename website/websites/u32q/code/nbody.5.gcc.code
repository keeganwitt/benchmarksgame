<span class="hl kwb">static</span> v2df dtdt <span class="hl opt">= {</span><span class="hl num">0.01</span><span class="hl opt">,</span> <span class="hl num">0.01</span><span class="hl opt">};</span>

<span class="hl kwb">static void</span> <span class="hl kwd">advance</span><span class="hl opt">(</span><span class="hl kwb">int</span> q<span class="hl opt">)</span>
<span class="hl opt">{</span>
  <span class="hl kwb">struct</span> planet <span class="hl opt">*</span>b<span class="hl opt">, *</span>b2<span class="hl opt">;</span>
  v2df dxdy<span class="hl opt">,</span> dz00<span class="hl opt">,</span> tsquare<span class="hl opt">,</span> distance2<span class="hl opt">,</span> magmag<span class="hl opt">;</span>
