         buffer <span class="hl kwb">replaceFrom:</span> <span class="hl num">1</span> <span class="hl kwb">to:</span> aLength <span class="hl kwb">with:</span> <span class="hl kwa">self</span> <span class="hl kwb">startingAt:</span> i<span class="hl opt">.</span>
         answer <span class="hl opt">:</span><span class="hl kwb">=</span> aDictionary
            <span class="hl kwb">at:</span> buffer
            <span class="hl kwb">putValueOf:</span> <span class="hl opt">[</span><span class="hl kwd">:sum</span> | sum <span class="hl kwb">+</span> <span class="hl num">1</span><span class="hl opt">]</span>
            <span class="hl kwb">ifAbsentPutValueOf:</span> <span class="hl num">1</span><span class="hl opt">.</span>
         answer <span class="hl kwb">=</span> <span class="hl num">1</span> <span class="hl kwb">ifTrue:</span> <span class="hl opt">[</span>buffer <span class="hl opt">:</span><span class="hl kwb">=</span> <span class="hl kwc">String</span> <span class="hl kwb">new:</span> aLength<span class="hl opt">].</span>
