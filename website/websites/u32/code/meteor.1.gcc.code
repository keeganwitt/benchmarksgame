   <span class="hl kwd">solve</span><span class="hl opt">(</span><span class="hl num">0</span><span class="hl opt">,</span> <span class="hl num">0</span><span class="hl opt">);</span>
   <span class="hl kwd">printf</span><span class="hl opt">(</span><span class="hl str">&quot;%d solutions found</span><span class="hl esc">\n\n</span><span class="hl str">&quot;</span><span class="hl opt">,</span> solution_count<span class="hl opt">);</span>
   <span class="hl kwd">qsort</span><span class="hl opt">(</span>solutions<span class="hl opt">,</span> solution_count<span class="hl opt">,</span> <span class="hl num">50</span> <span class="hl opt">*</span> <span class="hl kwa">sizeof</span><span class="hl opt">(</span><span class="hl kwb">signed char</span><span class="hl opt">),</span> solution_sort<span class="hl opt">);</span>
   <span class="hl kwd">pretty</span><span class="hl opt">(</span>solutions<span class="hl opt">[</span><span class="hl num">0</span><span class="hl opt">]);</span>
   <span class="hl kwd">pretty</span><span class="hl opt">(</span>solutions<span class="hl opt">[</span>solution_count<span class="hl opt">-</span><span class="hl num">1</span><span class="hl opt">]);</span>
   <span class="hl kwa">return</span> <span class="hl num">0</span><span class="hl opt">;</span>
<span class="hl opt">}</span>
